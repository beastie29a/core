{
  "services": {
    "start_program": "mdi:play",
    "select_program": "mdi:form-select",
    "pause_program": "mdi:pause",
    "resume_program": "mdi:play-pause",
    "set_option_active": "mdi:gesture-tap",
    "set_option_selected": "mdi:gesture-tap",
    "change_setting": "mdi:cog",
    "get_data": "mdi:code-json"
  },
  "entity": {
    "sensor": {
      "door_sensor": {
        "default": "mdi:door",
        "state": {
          "closed": "mdi:door-closed",
          "open": "mdi:door-open",
          "locked": "mdi:door-closed-lock",
          "ajar": "mdi:door-closed-cancel"
        }
      },
      "alarm_sensor_fridge": {
        "default": "mdi:fridge",
        "state": {
          "off": "mdi:fridge",
          "present": "mdi:fridge-alert",
          "confirmed": "mdi:fridge-alert-outline"
        }
      },
      "alarm_sensor_freezer": {
        "default": "mdi:snowflake",
        "state": {
          "off": "mdi:snowflake",
          "present": "mdi:snowflake-alert",
          "confirmed": "mdi:snowflake-check"
        }
      },
      "alarm_sensor_temp": {
        "default": "mdi:thermometer",
        "state": {
          "off": "mdi:thermometer",
          "present": "mdi:thermometer-alert",
          "confirmed": "mdi:thermometer-check"
        }
      },
      "alarm_sensor_coffee_bean_container": {
        "default": "mdi:thermometer",
        "state": {
          "off": "mdi:thermometer",
          "present": "mdi:thermometer-alert",
          "confirmed": "mdi:thermometer-check"
        }
      },
      "alarm_sensor_coffee_water_tank": {
        "default": "mdi:water",
        "state": {
          "off": "mdi:water",
          "present": "mdi:water-alert",
          "confirmed": "mdi:water-check"
        }
      },
      "alarm_sensor_coffee_drip_tray": {
        "default": "mdi:tray",
        "state": {
          "off": "mdi:tray",
          "present": "mdi:tray-alert",
          "confirmed": "mdi:tray-full"
        }
      }
    }
  }
}
